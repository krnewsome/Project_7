ul.app--user--list
  each friend in friendsList
    li
      .circle--fluid
        .circle--fluid--cell.circle--fluid--primary
          a.app--tweet--author
            .app--avatar
              img(src= friend.userImage)
            h4 #{friend.rlName}
            p @#{friend.scrName}
        .circle--fluid--cell
          a.button.button-text Unfollow
